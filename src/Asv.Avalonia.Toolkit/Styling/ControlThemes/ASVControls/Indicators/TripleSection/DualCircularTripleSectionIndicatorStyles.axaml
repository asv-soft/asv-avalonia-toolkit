<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:indicators="clr-namespace:Asv.Avalonia.Toolkit.UI.Controls.Indicators">
    
    <Design.PreviewWith>
        <indicators:DualCircularTripleSectionIndicator 
            Width="400" 
            Height="200"
            Title="CRS \ CLR"
            LeftTopText="-90.0"
            LeftTopTitle="CLR Power, dBm"
           
            
            TopRightProgressLinePoint="100,2.5"
            TopRightProgressValue="-60"
            TopRightMax="-120"
            TopRightMin="20"
            
            TopLeftProgressLinePoint="100,2.5"
            TopLeftProgressValue="-120"
            TopLeftMax="-120"
            TopLeftMin="20"
            
            LeftBottomLeftText="+40.0"
            LeftBottomLeftTitle="DDM,%"
            LeftBottomLeftStatus="Success"
            
            LeftBottomRightText="-80.8"
            LeftBottomRightTitle="SDM,%"
            LeftBottomRightStatus="Warning"
            
            RightTopText="-110.0"
            RightTopTitle="CRS Power, dBm"
            
            RightBottomLeftText="+40.0"
            RightBottomLeftTitle="DDM,%"
            RightBottomLeftStatus="Success"
            
            RightBottomRightText="-80.8"
            RightBottomRightTitle="SDM,%"
            RightBottomRightStatus="Unknown"
            
            BottomStatusText="Warning"
            BottomStatus="Warning"/>
    </Design.PreviewWith>
    
    <ControlTheme x:Key="{x:Type indicators:DualCircularTripleSectionIndicator}" TargetType="indicators:DualCircularTripleSectionIndicator">
        <!-- Set Defaults -->
        <Setter Property="BorderBrush" Value="{DynamicResource IndicatorBorderBrush}"/>
        <Setter Property="Template">
            <ControlTemplate>
                <Viewbox>
                    <Panel>
                        <Canvas HorizontalAlignment="Left" Width="200" Height="100">
                            <Arc
                                x:Name="TopLeftArc"
                                Canvas.Left="2"
                                Canvas.Top="2"
                                Height="96"
                                Width="96"
                                StartAngle="190"
                                Stretch="None"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{TemplateBinding BorderThickness}"
                                SweepAngle="80"
                            />    
                            <Line 
                                x:Name="TopLeftLine"
                                StartPoint="50,2.5"
                                EndPoint="100,2.5"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{TemplateBinding BorderThickness}"/>
                            
                            <Arc
                                x:Name="TopRightArc"
                                Canvas.Left="102"
                                Canvas.Top="2"
                                Height="96"
                                Width="96"
                                StartAngle="-10"
                                Stretch="None"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{TemplateBinding BorderThickness}"
                                SweepAngle="-80"/> 
                           
                            <Line x:Name="TopRightLine"
                                  StartPoint="100,2.5"
                                  EndPoint="150,2.5"
                                  Stroke="{TemplateBinding BorderBrush}"
                                  StrokeThickness="{TemplateBinding BorderThickness}"/>
                            
                            <Arc x:Name="BottomLeftArc"
                                 Canvas.Left="2"
                                 Canvas.Top="2"
                                 Height="96"
                                 Width="96"
                                 StartAngle="90"
                                 Stretch="None"
                                 Stroke="{TemplateBinding BorderBrush}"
                                 StrokeThickness="{TemplateBinding BorderThickness}"
                                 SweepAngle="80"/>    
                            
                            <Line 
                                x:Name="BottomLeftLine"
                                StartPoint="50,97.5"
                                EndPoint="100,97.5"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{TemplateBinding BorderThickness}"/>
                            
                            <Arc
                                x:Name="BottomRightArc"
                                Canvas.Left="102"
                                Canvas.Top="2"
                                Height="96"
                                Width="96"
                                StartAngle="10"
                                Stretch="None"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{TemplateBinding BorderThickness}"
                                SweepAngle="80"/> 
                            
                            <Line 
                                x:Name="BottomRightLine"
                                StartPoint="100,97.5"
                                EndPoint="150,97.5"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{TemplateBinding BorderThickness}"/>
                            <Line 
                                StartPoint="10,50"
                                EndPoint="190,50"
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{TemplateBinding BorderThickness}"/>
                            <Line 
                                StartPoint="50,50"
                                EndPoint="50,90" 
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{TemplateBinding BorderThickness}"/>
                            <Line 
                                StartPoint="100,20"
                                EndPoint="100,80" 
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{TemplateBinding BorderThickness}"/>
                            <Line 
                                StartPoint="150,50"
                                EndPoint="150,90" 
                                Stroke="{TemplateBinding BorderBrush}"
                                StrokeThickness="{TemplateBinding BorderThickness}"/>
                            
                             <Line x:Name="TopLeftProgressLine" 
                                  StartPoint="100,2.5" 
                                  EndPoint="{TemplateBinding TopLeftProgressLinePoint}" 
                                  Stroke="{TemplateBinding BorderBrush}"
                                  StrokeThickness="5"/> 
                            
                            <Arc x:Name="TopLeftProgressArc"
                                 Height="100"
                                 StartAngle="-90"
                                 Stretch="None"
                                 StrokeThickness="5"
                                 SweepAngle="{TemplateBinding TopLeftProgressAngle}"
                                 Width="100"/>
                            
                            <Line x:Name="TopRightProgressLine"
                                  StartPoint="100,2.5" 
                                  EndPoint="{TemplateBinding TopRightProgressLinePoint}"  
                                  Stroke="{TemplateBinding BorderBrush}"
                                  StrokeThickness="5" />
                            
                            <Arc x:Name="TopRightProgressArc"
                                 Canvas.Left="100"
                                 Height="100"
                                 StartAngle="270"
                                 Stretch="None"
                                 StrokeThickness="5"
                                 SweepAngle="{TemplateBinding TopRightProgressAngle}"
                                 Width="100"/>
                            
                            
                        </Canvas> 
                        
                        <Grid  RowDefinitions="35,10,8,20,10" ColumnDefinitions="*,5,*,5,*,5,*">
                            <TextBlock 
                                Grid.Row="0" 
                                Grid.ColumnSpan="3" 
                                Grid.Column="2" 
                                Foreground="{DynamicResource IndicatorTextBrush}"
                                HorizontalAlignment="Center"
                                Margin="0,4,0,0"
                                VerticalAlignment="Top" 
                                FontSize="10"
                                Text="{TemplateBinding Title}"/>
                            <TextBlock 
                                x:Name="BottomStatusText"
                                Grid.Row="4"
                                Grid.ColumnSpan="3"
                                Grid.Column="2"
                                HorizontalAlignment="Center"
                                Margin="0,9,0,0"
                                VerticalAlignment="Bottom" 
                                FontSize="10"
                                Text="{TemplateBinding BottomStatusText}"/>
                            <TextBlock 
                                x:Name="LeftTopTextBlock"
                                FontWeight="Bold" 
                                Grid.Row="0"
                                Grid.ColumnSpan="3"
                                Grid.Column="0"
                                HorizontalAlignment="Center" 
                                VerticalAlignment="Bottom" 
                                Text="{TemplateBinding LeftTopText}"/>
                            <TextBlock 
                                Foreground="{DynamicResource IndicatorTextBrush}"
                                Grid.Row="1"
                                FontSize="8"
                                Grid.ColumnSpan="3"
                                Grid.Column="0"
                                HorizontalAlignment="Center" 
                                VerticalAlignment="Bottom" Text="{TemplateBinding LeftTopTitle}"/>
                            <TextBlock 
                                x:Name="LeftBottomLeftTextBlock"
                                Grid.Row="3" Grid.Column="0" 
                                FontSize="12" 
                                HorizontalAlignment="Right" 
                                VerticalAlignment="Top" 
                                Text="{TemplateBinding LeftBottomLeftText}"/>
                            <TextBlock 
                                Foreground="{DynamicResource IndicatorTextBrush}"
                                Margin="0,-3,2,0" 
                                Grid.Row="4" 
                                FontSize="7"
                                Grid.Column="0" 
                                HorizontalAlignment="Right" 
                                VerticalAlignment="Top" 
                                Text="{TemplateBinding LeftBottomLeftTitle}"/>
                            <TextBlock 
                                x:Name="LeftBottomRightTextBlock"
                                Margin="2,0,0,0" 
                                Grid.Row="3" 
                                Grid.Column="2" 
                                FontSize="12" 
                                HorizontalAlignment="Left" 
                                VerticalAlignment="Top"
                                Text="{TemplateBinding LeftBottomRightText}"/>
                            <TextBlock 
                                Foreground="{DynamicResource IndicatorTextBrush}"
                                Margin="5,-3,0,0" 
                                Grid.Row="4" 
                                FontSize="7" 
                                Grid.Column="2" 
                                HorizontalAlignment="Left" 
                                VerticalAlignment="Top" 
                                Text="{TemplateBinding LeftBottomRightTitle}"/>
                            <TextBlock 
                                x:Name="RightTopTextBlock"
                                FontWeight="Bold" 
                                Grid.Row="0" 
                                Grid.ColumnSpan="3" 
                                Grid.Column="4" 
                                HorizontalAlignment="Center" 
                                VerticalAlignment="Bottom" 
                                Text="{TemplateBinding RightTopText}"/>
                            <TextBlock 
                                Foreground="{DynamicResource IndicatorTextBrush}" 
                                Grid.Row="1" 
                                FontSize="8" 
                                Grid.ColumnSpan="3" 
                                Grid.Column="4" 
                                HorizontalAlignment="Center" 
                                VerticalAlignment="Bottom" 
                                Text="{TemplateBinding RightTopTitle}"/>
                            <TextBlock 
                                x:Name="RightBottomLeftTextBlock"
                                Margin="0,0,2,0" 
                                Grid.Row="3" 
                                Grid.Column="4" 
                                FontSize="12" 
                                HorizontalAlignment="Right" 
                                VerticalAlignment="Top" 
                                Text="{TemplateBinding RightBottomLeftText}"/>
                            <TextBlock 
                                Foreground="{DynamicResource IndicatorTextBrush}" 
                                Margin="0,-3,2,0" 
                                Grid.Row="4" 
                                FontSize="7" 
                                Grid.Column="4" 
                                HorizontalAlignment="Right" 
                                VerticalAlignment="Top" 
                                Text="{TemplateBinding RightBottomLeftTitle}"/>
                            <TextBlock 
                                x:Name="RightBottomRightTextBlock"
                                Margin="3,0,0,0"
                                Grid.Row="3" 
                                Grid.Column="6" 
                                FontSize="12" 
                                HorizontalAlignment="Left" 
                                VerticalAlignment="Top" 
                                Text="{TemplateBinding RightBottomRightText}"/>
                            <TextBlock 
                                Foreground="{DynamicResource IndicatorTextBrush}" 
                                Margin="3,-3,0,0" 
                                Grid.Row="4" 
                                FontSize="7" 
                                Grid.Column="6" 
                                HorizontalAlignment="Left" 
                                VerticalAlignment="Top" 
                                Text="{TemplateBinding RightBottomRightTitle}"/>
                        </Grid>
                    </Panel>
                </Viewbox>
            </ControlTemplate>
        </Setter>
        
        
